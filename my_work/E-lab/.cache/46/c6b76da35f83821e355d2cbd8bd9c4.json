illium Web\", sans-serif;\r\n  font-size: 16px;\r\n  color: var(--dark-blue);\r\n}\r\n\r\nbody {\r\n  height: 100vh;\r\n  background-image: url(images/bg-image.jpg);\r\n  background-attachment: fixed;\r\n  background-size: cover;\r\n  background-repeat: no-repeat;\r\n}\r\n\r\nsection {\r\n  min-height: 100vh;\r\n  position: relative;\r\n}\r\n\r\na {\r\n  text-decoration: none;\r\n}\r\n\r\n.container {\r\n  margin: 0 auto;\r\n  background-color: #f2f2f2;\r\n  width: 85%;\r\n  height: 100vh;\r\n  position: relative;\r\n}\r\n\r\n.container.intro {\r\n  position: absolute;\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n  border-top-left-radius: 2rem;\r\n  border-top-right-radius: 2rem;\r\n  height: 80vh;\r\n  bottom: 0;\r\n}\r\n.intro-title {\r\n  padding: 0 3.5rem;\r\n  display: inline-block;\r\n  margin-top: 6rem;\r\n}\r\n.container-content {\r\n  position: absolute;\r\n  bottom: 0;\r\n  width: 100%;\r\n  height: calc(100vh - 120px);\r\n}\r\n\r\nh1 {\r\n  font-size: 2.5rem;\r\n  text-transform: uppercase;\r\n  padding: 2rem 3rem;\r\n  text-decoration: underline;\r\n}\r\n\r\nhr {\r\n  color: #777;\r\n  border-top: none;\r\n  width: 85%;\r\n  margin: 0 auto;\r\n}\r\n\r\n.inner-content {\r\n  margin: 0 auto;\r\n  width: 94%;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.inner-content img {\r\n  width: 450px;\r\n  height: auto;\r\n  border-radius: 2rem;\r\n}\r\n\r\n.text {\r\n  font-size: 1.3rem;\r\n  width: 50%;\r\n}\r\n\r\n.c-form {\r\n  position: absolute;\r\n  top: 55%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  width: 80%;\r\n}\r\nform {\r\n  width: 50%;\r\n}\r\n\r\n.form-group {\r\n  margin-top: 1.5rem;\r\n  display: flex;\r\n  flex-direction: column;\r\n  width: 100%;\r\n}\r\n\r\nlabel {\r\n  font-size: 1.7rem;\r\n}\r\ninput {\r\n  border: none;\r\n  height: 50px;\r\n  width: 100%;\r\n}\r\n\r\ntextarea {\r\n  border: n